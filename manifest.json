{
  "name": "ReadEase",
  "description": "Extension to ease reading for individuals with dyslexia",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js":[
            "app.js"
        ]
    }
  ],
  "key": 
  "",
  "icons": {
    "128": "Readease_128.png",
    "48": "Readease_48.png",
    "32": "Readease_32.png",
    "16": "Readease_16.png"
   },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
        "128": "Readease_128.png",
        "48": "Readease_48.png",
        "32": "Readease_32.png",
        "16": "Readease_16.png"
      },
    "default_popup": "login-popup.html"
  },
  "permissions": [
    "identity",
    "storage",
    "tabs",
    "activeTab",
    "webNavigation"
  ]
}
